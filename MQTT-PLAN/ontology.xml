  <rdf:Description rdf:about="http://w3id.org/mqtt-plan#Connect">
    <rdfs:label xml:lang="en">Connect</rdfs:label>
    <rdfs:comment xml:lang="en">A step describing a planned activity of connecting a client. The client is described  as the mqtt-prov:Sender of the mqtt-prov:ControlPacket which triggers this action.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://w3id.org/mqtt-plan#BrokerAction"/>
